<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="OrangeHRM Suite" thread-count="5" parallel="classes">


	<groups>
		<run>
			<include name="smoke"></include>
			<include name="regr"></include>
		</run>
	</groups>

	<parameter name="browser" value="edge"></parameter>

	<test name="Login UI Test edge">
		<classes>
			<class name="com.eq.test.LoginUITest" />
			<class name="com.eq.test.LoginTest" />
			<class name="com.eq.test.EmployeeTest" />
		</classes>
	</test>


</suite> 
